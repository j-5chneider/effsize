	file = {Field et al. - 2018 - The Effect of Preregistration on Trust in Empirica.pdf:C\:\\Users\\Schneider\\Zotero\\storage\\KEMUAYZ5\\Field et al. - 2018 - The Effect of Preregistration on Trust in Empirica.pdf:application/pdf},
