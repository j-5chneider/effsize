	note = {Publisher: Forum Qualitative Sozialforschung / Forum: Qualitative Social Research},
	keywords = {Theorie-Praxis-Bezug; Lehrer*in­nenbildung; Hoch­schullehrende; epistemologische Überzeugungen; Grounded-Theory-Methodologie; problemzentriertes Interview; Fallanalyse},
	pages = {Art. 4},
