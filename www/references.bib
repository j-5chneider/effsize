	keywords = {effect sizes: practical relevance},
	file = {Eingereichte Version:C\:\\Users\\Schneider\\Zotero\\storage\\HRN7ZWVJ\\Anvari et al. - 2021 - Evaluating the practical relevance and significanc.pdf:application/pdf},
}

