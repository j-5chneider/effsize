	keywords = {Affect, Affective   persistence, Affective computing, attention, autonomy, autotutor, Cognitive-affective states, construction, dialogue, Educational games, emotion, Intelligent tutoring systems, recognition, risk-taking, user frustration},
	pages = {223--241},
