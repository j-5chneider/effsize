	author = {Scheiter, Katharina},
	month = oct,
	year = {2021},
	pages = {1039--1060},
	file = {Volltext:C\:\\Users\\Schneider\\Zotero\\storage\\TGDTUI36\\Scheiter - 2021 - Lernen und Lehren mit digitalen Medien Eine Stand.pdf:application/pdf},
}

