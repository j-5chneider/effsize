	keywords = {Cognitive Style, Computer Assisted Instruction, Electronic Learning, English (Second Language), Gender Differences, Individualized Instruction, Instructional Design, Intelligent Tutoring Systems, Learning Motivation, Learning Processes, Pretests Posttests, Programming, Second Language Instruction, Student Characteristics},
